import java.rmi.*;

public class MyClient{  
	
	public static void main(String args[]){  
		try{  
			Adder stub=new AdderRemote("myProg-" + args[0].toString());  
			Naming.rebind("rmi://localhost:5000/myProg-" + args[0].toString(),stub);
			stub.hasLock = 1;
		}catch(Exception e){
			
		}
	}
	
}  